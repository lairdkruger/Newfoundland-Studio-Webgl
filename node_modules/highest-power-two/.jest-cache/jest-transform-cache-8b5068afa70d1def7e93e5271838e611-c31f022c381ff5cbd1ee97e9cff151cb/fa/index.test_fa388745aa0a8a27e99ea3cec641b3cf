import { should } from 'chai';
import highestPowerTwo from 'src/index';

describe('#highestPowerTwo', function () {
  it('returns 0 for numbers below zero', function () {
    highestPowerTwo(-5).should.equal(0);
  });

  it('correctly returns the next highest power of two', function () {
    highestPowerTwo(2).should.equal(2);
    highestPowerTwo(14).should.equal(8);
    highestPowerTwo(19).should.equal(16);
    highestPowerTwo(51).should.equal(32);
  });
});
